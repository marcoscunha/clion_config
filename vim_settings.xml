      <register name="6" type="4">
        <text>bool_t ble_rgb ( uint8_t R, uint8_t G, uint8_t B, bool_t force )
{
    uint8_t frame[ BLE_MAX_FRAME ];
    uint8_t value[ BLE_MAX_PAYLOAD ];
    uint8_t size = 0;
    static uint8_t current_value[ 3 ];
    if ( current_value[ 0 ] != R ||
         current_value[ 1 ] != G ||
         current_value[ 2 ] != B ||
         force )
    {
        value[ 0 ] = R;
        value[ 1 ] = G;
        value[ 2 ] = B;
        current_value[ 0 ] = R;
        current_value[ 1 ] = G;
        current_value[ 2 ] = B;
        size = ble_bundle ( BLE_RGB, 0x03, value, frame );
        return ble_send_array ( frame, size, FALSE, NULL );
    }
    return TRUE;
}</text>
